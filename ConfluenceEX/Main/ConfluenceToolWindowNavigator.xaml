<UserControl x:Class="ConfluenceEX.Main.ConfluenceToolWindowNavigator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:local="clr-namespace:ConfluenceEX.View"
            Height="350" Width="525">

    <UserControl.Resources>
        <DataTemplate DataType="{x:Type local:ContentListView}">
            <local:ContentListView/>
        </DataTemplate>
    </UserControl.Resources>

    <ContentControl x:Name="Pages" DockPanel.Dock="Top" Content="{Binding SelectedView}"/>
    
</UserControl>
